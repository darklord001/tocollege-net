<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.apress.progwt.client.domain">

	<class name="SchoolAndAppProcess" table="SchoolAndAppProcess">
		<meta attribute="generated-class">com.apress.progwt.client.domain.generated.AbstractSchoolAndAppProcess</meta>
		<meta attribute="scope-class">public abstract</meta>

		<id name="id" type="long" column="id">
			<generator class="native"></generator>
		</id>

        <many-to-one name="school" column="school_id" class="School"></many-to-one>		
        <many-to-one name="application" column="application_id" class="ApplicationProcess"></many-to-one>       
	
	</class>

</hibernate-mapping>