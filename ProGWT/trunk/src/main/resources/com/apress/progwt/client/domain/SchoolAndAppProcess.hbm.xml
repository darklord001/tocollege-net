<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.apress.progwt.client.domain">

	<class name="SchoolAndAppProcess" table="SchoolAndAppProcess">
		<meta attribute="generated-class">com.apress.progwt.client.domain.generated.AbstractSchoolAndAppProcess</meta>
		<meta attribute="scope-class">public abstract</meta>

		<id name="id" type="long" column="id">
			<generator class="native"></generator>
		</id>
       
        <property name="sortOrder" column="sort_order" type="int"/>

<!-- <many-to-one name="user" column="user_id" class="User"/> -->
        <many-to-one name="user" column="user_id" class="User" insert="false" update="false" />        
        <many-to-one name="school" column="school_id" class="School" cascade="none" lazy="false"></many-to-one>		
        <many-to-one name="application" column="application_id" class="ApplicationProcess" cascade="all" lazy="false"></many-to-one>       
	
	</class>

</hibernate-mapping>